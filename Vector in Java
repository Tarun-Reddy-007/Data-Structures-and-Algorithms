import java.util.Vector;

public class VectorExample {
    public static void main(String[] args) {
        Vector<Integer> vector = new Vector<>();
        vector.add(1);
        vector.add(2);
        vector.add(3);
        System.out.println(vector.get(1));
        vector.remove(1); 
        for (Integer value : vector) {
            System.out.println(value);
        }
    }
}
